.PHONY: help install setup run test clean docker-up docker-down logs status lint format\n\nhelp:\n\t@echo \"FactMarrow Development Commands\"\n\t@echo \"================================\"\n\t@grep -E '^[a-zA-Z_-]+:.*?## .*$$' $(MAKEFILE_LIST) | sort | awk 'BEGIN {FS = \":.*?## \"}; {printf \"\\033[36m%-20s\\033[0m %s\\n\", $$1, $$2}'\n\ninstall: ## Install Python dependencies\n\tpip install -r requirements.txt\n\nsetup: ## Complete setup (install + docker)\n\t$(MAKE) install\n\t$(MAKE) docker-up\n\nrun: ## Run FactMarrow Cagent\n\tcagent run agents/factmarrow_agents.yaml\n\ntest: ## Run tests with coverage\n\tpytest tests/ -v || true\n\nlint: ## Run linting checks\n\tflake8 src tests || true\n\nformat: ## Format code\n\tblack src tests || true\n\tdocker-up: ## Start Docker services\n\tdocker-compose up -d\n\t@echo \"Waiting for services to start...\"\n\tsleep 5\n\t@echo \"Services status:\"\n\tdocker-compose ps\n\ndocker-down: ## Stop Docker services\n\tdocker-compose down\n\ndocker-clean: ## Remove containers and volumes\n\tdocker-compose down -v\n\nlogs: ## View Docker logs\n\tdocker-compose logs -f\n\nstatus: ## Show setup status\n\t@echo \"=== FactMarrow Status ===\"\n\t@echo \"Cagent: $$(cagent --version 2>/dev/null || echo 'NOT INSTALLED')\"\n\t@echo \"Docker: $$(docker --version)\"\n\t@echo \"Python: $$(python3 --version)\"\n\t@echo \"\"\n\t@docker-compose ps 2>/dev/null || echo \"Docker services not running\"\n\nclean: ## Clean up cache and build files\n\tfind . -type d -name __pycache__ -exec rm -r {} + 2>/dev/null || true\n\tfind . -type f -name '*.pyc' -delete\n\trm -rf build/ dist/ *.egg-info/ .pytest_cache/ .coverage htmlcov/\n\n.DEFAULT_GOAL := help\n"